@charset "UTF-8";

$r_750:640/750/2/32;
@function rem($px){
	@return $px * $r_750 * 1rem;
};

{{#sprites}}
.{{name}}{
  display: block;
  background-image: url({{{image}}});
  background-size: {{px.total_width}} {{px.total_height}};
  background-position: {{px.offset_x}} {{px.offset_y}};
  width: {{px.width}};
  height: {{px.height}};
}
{{/sprites}}
